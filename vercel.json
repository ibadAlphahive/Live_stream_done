{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/next"
        }
    ],
    "installCommand": "npm install --legacy-peer-deps",
    "buildCommand": "npm run build",
    "outputDirectory": ".next",
    "functions": {
        "api/**/*.js": {
            "runtime": "nodejs18.x"
        },
        "api/**/*.ts": {
            "runtime": "nodejs18.x"
        }
    }
}